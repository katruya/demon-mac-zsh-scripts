# MY ALIASES

# Edit .zshrc
alias ezrc='vim ~/.zshrc && cd -'
alias szrc='source ~/.zshrc'

# Edit .hyper.js
alias hyjs='vim ~/.hyper.js && cd -'

# System
alias lla='ls -la'
alias cls='clear'
alias mkf='touch'
alias mkd='mkdir'

# FUCK
alias fuck='sudo'
alias fuckit='sudo su'

# PG f**k
alias fk='sudo'
alias fkit='sudo su'

# git commands
alias gnt='git init'

alias ga='git add'

alias gaa='git add .'

alias gc='git commit -m'

alias glg='git log'

alias gst='git status'

alias gph='git push'

# node/npm commands
alias js='node'
alias nit='npm init'
alias ni='npm install'
alias nps='npm start'
alias npt='npm test'

# Compile & Run - C++
alias cpcr='g++ -I -std=c++12 *.cpp -o app && ./app'

# brew commands
alias bi='brew install'
